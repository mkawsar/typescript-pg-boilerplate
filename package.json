        "@types/jsonwebtoken": "^9.0.6",
        "@types/memory-cache": "^0.2.5",
        "@types/node": "^16.11.10",
        "ts-node": "10.9.1",
        "typescript": "4.5.2"
   },
   "dependencies": {
        "bcrypt": "^5.1.1",
        "concurrently": "^8.2.2",
